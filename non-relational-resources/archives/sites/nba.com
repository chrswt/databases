  ;(function _metadata_init(w) {

    w.turner_metadata = {

      /* ------------------------------------------------------------------- *
       * Basic Information
       * ------------------------------------------------------------------- */
      analyticsVersion : "2.0",
      appName          : "app:nba - desktop",
      entity           : "league:nba",
      turnerId         : "aa41822-1193725393-1389694715530-1",

      /* ------------------------------------------------------------------- *
       * Game - need live game to review this
       * ------------------------------------------------------------------- */
      gameId      : "none",
      gameQuarter : "none",
      gameState   : "none",

      /* ------------------------------------------------------------------- *
       * Entity
       * ------------------------------------------------------------------- */
      playerId : "none",
      teamId   : "none",

      /* ------------------------------------------------------------------- *
       * Branding - awaiting final ads code push
       * ------------------------------------------------------------------- */
      sponsor : "none",

      /* ------------------------------------------------------------------- *
       * Article Information
       * ------------------------------------------------------------------- */
      articleCategory    : "none",
      articleTitle       : "none",
      articleAuthor      : "none",
      articlePublishDate : "none",
      articleTags        : "none"
    }

    document.addEventListener( "DOMContentLoaded", function( event ) {

      w.turner_metadata.playerId = ( jQuery('.nba-player-header__button button').attr('data-followid') || "none" );
      w.turner_metadata.teamId = ( jQuery('.nba-team-header__button--follow').attr('data-followid') || "none" );

      w.turner_metadata.articleTitle = ( jQuery('.article__detail--header-text').html() || "none" );
      w.turner_metadata.articlePublishDate = ( jQuery('.article__detail--date').text() || "none" );

      let html = jQuery('.article__detail--writer-profile p').first().html();

      w.turner_metadata.articleAuthor = html ? ( html.replace( /<span[^>]*>[\s\S]+<\/span>/, "" ) || "none" ) : "none" ;

      /* ------------------------------------------------------------------- *
       * Game - here's the logic to determine the current game state
       * ------------------------------------------------------------------- */
      setInterval(function () {

        w.turner_metadata.gameId = ( jQuery('a[ng-reflect-href="' + window.location.pathname + '"]').attr('metadata-gameid') || "none" );

        var state = ( jQuery('[ng-reflect-href="' + window.location.pathname + '"]').attr('metadata-gamestatus') || "none" );

        if (state == "1") {

          w.turner_metadata.gameState = "pre";

        } else if (state == "3") {

          w.turner_metadata.gameState = "post";

        } else if (state == "2") {

          w.turner_metadata.gameState = "live";
          w.turner_metadata.gameQuarter = ( jQuery('a[ng-reflect-href="' + window.location.pathname + '"]').attr('metadata-period') || "none" );
        }

      }, 10000 );

    });

  }(window));

